<h1> Schedule</h1>
<div class="d-flex flex-wrap">
<% for (course of courses) {   
    %>

    <div class="flex-container p-2 m-2 border border-info border-5">
        Course: <%= course['subj'] %> <%= course['num'] %> <%= course['title'] %> 
        <br>
        Term: <%= course['term'] %> 
        <br>
        Instructor: <%= course['inst'] %>
        <br>
        Enrolled: <%= course['enr'] %> enrolled
        <br> 
        
        
    </div>
    <% } %>
</div>

